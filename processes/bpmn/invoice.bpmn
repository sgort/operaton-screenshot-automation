<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
                  id="Definitions_Invoice"
                  targetNamespace="http://operaton.org/examples/invoice"
                  exporter="Camunda Modeler"
                  exporterVersion="5.0.0">
  
  <bpmn:process id="invoice" name="Invoice Receipt" isExecutable="true" camunda:historyTimeToLive="30">
    
    <bpmn:startEvent id="StartEvent_InvoiceReceived" name="Invoice&#10;received">
      <bpmn:outgoing>Flow_ToAssignApprover</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:sequenceFlow id="Flow_ToAssignApprover" sourceRef="StartEvent_InvoiceReceived" targetRef="Task_AssignApprover" />
    
    <bpmn:businessRuleTask id="Task_AssignApprover" name="Assign Approver" camunda:resultVariable="approver" camunda:decisionRef="invoice-assign-approver" camunda:mapDecisionResult="singleEntry">
      <bpmn:incoming>Flow_ToAssignApprover</bpmn:incoming>
      <bpmn:outgoing>Flow_ToApproveInvoice</bpmn:outgoing>
    </bpmn:businessRuleTask>
    
    <bpmn:sequenceFlow id="Flow_ToApproveInvoice" sourceRef="Task_AssignApprover" targetRef="Task_ApproveInvoice" />
    
    <bpmn:userTask id="Task_ApproveInvoice" name="Approve Invoice" camunda:assignee="${approver}" camunda:formKey="embedded:app:forms/approve-invoice.html">
      <bpmn:extensionElements>
        <camunda:taskListener delegateExpression="${approverAssignment}" event="create" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToApproveInvoice</bpmn:incoming>
      <bpmn:incoming>Flow_ReviewToApprove</bpmn:incoming>
      <bpmn:outgoing>Flow_ToGatewayApproved</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:sequenceFlow id="Flow_ToGatewayApproved" sourceRef="Task_ApproveInvoice" targetRef="Gateway_InvoiceApproved" />
    
    <bpmn:exclusiveGateway id="Gateway_InvoiceApproved" name="Invoice&#10;approved?">
      <bpmn:incoming>Flow_ToGatewayApproved</bpmn:incoming>
      <bpmn:outgoing>Flow_Approved</bpmn:outgoing>
      <bpmn:outgoing>Flow_NotApproved</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:sequenceFlow id="Flow_Approved" name="yes" sourceRef="Gateway_InvoiceApproved" targetRef="Task_PrepareBankTransfer">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_NotApproved" name="no" sourceRef="Gateway_InvoiceApproved" targetRef="Task_ReviewInvoice">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!approved}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:userTask id="Task_ReviewInvoice" name="Review Invoice" camunda:assignee="demo" camunda:formKey="embedded:app:forms/review-invoice.html">
      <bpmn:incoming>Flow_NotApproved</bpmn:incoming>
      <bpmn:outgoing>Flow_ToGatewayReview</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:sequenceFlow id="Flow_ToGatewayReview" sourceRef="Task_ReviewInvoice" targetRef="Gateway_ReviewOutcome" />
    
    <bpmn:exclusiveGateway id="Gateway_ReviewOutcome" name="Review&#10;outcome?">
      <bpmn:incoming>Flow_ToGatewayReview</bpmn:incoming>
      <bpmn:outgoing>Flow_ReviewToApprove</bpmn:outgoing>
      <bpmn:outgoing>Flow_ReviewToReject</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:sequenceFlow id="Flow_ReviewToApprove" name="clarified" sourceRef="Gateway_ReviewOutcome" targetRef="Task_ApproveInvoice">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${clarified}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_ReviewToReject" name="not clarified" sourceRef="Gateway_ReviewOutcome" targetRef="EndEvent_InvoiceRejected">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!clarified}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:userTask id="Task_PrepareBankTransfer" name="Prepare Bank Transfer" camunda:candidateGroups="accounting" camunda:formKey="embedded:app:forms/prepare-bank-transfer.html">
      <bpmn:incoming>Flow_Approved</bpmn:incoming>
      <bpmn:outgoing>Flow_ToArchive</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:sequenceFlow id="Flow_ToArchive" sourceRef="Task_PrepareBankTransfer" targetRef="Task_ArchiveInvoice" />
    
    <bpmn:serviceTask id="Task_ArchiveInvoice" name="Archive Invoice" camunda:delegateExpression="${archiveInvoice}">
      <bpmn:incoming>Flow_ToArchive</bpmn:incoming>
      <bpmn:outgoing>Flow_ToEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:sequenceFlow id="Flow_ToEnd" sourceRef="Task_ArchiveInvoice" targetRef="EndEvent_InvoicePaid" />
    
    <bpmn:endEvent id="EndEvent_InvoicePaid" name="Invoice&#10;paid">
      <bpmn:incoming>Flow_ToEnd</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:endEvent id="EndEvent_InvoiceRejected" name="Invoice&#10;rejected">
      <bpmn:incoming>Flow_ReviewToReject</bpmn:incoming>
    </bpmn:endEvent>
    
  </bpmn:process>
  
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="invoice">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_InvoiceReceived">
        <dc:Bounds x="179" y="159" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="172" y="202" width="50" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_AssignApprover_di" bpmnElement="Task_AssignApprover">
        <dc:Bounds x="270" y="137" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ApproveInvoice_di" bpmnElement="Task_ApproveInvoice">
        <dc:Bounds x="430" y="137" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_InvoiceApproved_di" bpmnElement="Gateway_InvoiceApproved" isMarkerVisible="true">
        <dc:Bounds x="595" y="152" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="595" y="209" width="50" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_PrepareBankTransfer_di" bpmnElement="Task_PrepareBankTransfer">
        <dc:Bounds x="710" y="137" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ReviewInvoice_di" bpmnElement="Task_ReviewInvoice">
        <dc:Bounds x="570" y="280" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_ReviewOutcome_di" bpmnElement="Gateway_ReviewOutcome" isMarkerVisible="true">
        <dc:Bounds x="455" y="295" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="455" y="352" width="50" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ArchiveInvoice_di" bpmnElement="Task_ArchiveInvoice">
        <dc:Bounds x="870" y="137" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_InvoicePaid_di" bpmnElement="EndEvent_InvoicePaid">
        <dc:Bounds x="1032" y="159" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1031" y="202" width="38" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_InvoiceRejected_di" bpmnElement="EndEvent_InvoiceRejected">
        <dc:Bounds x="302" y="302" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="298" y="345" width="45" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_ToAssignApprover_di" bpmnElement="Flow_ToAssignApprover">
        <di:waypoint x="215" y="177" />
        <di:waypoint x="270" y="177" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ToApproveInvoice_di" bpmnElement="Flow_ToApproveInvoice">
        <di:waypoint x="370" y="177" />
        <di:waypoint x="430" y="177" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ToGatewayApproved_di" bpmnElement="Flow_ToGatewayApproved">
        <di:waypoint x="530" y="177" />
        <di:waypoint x="595" y="177" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Approved_di" bpmnElement="Flow_Approved">
        <di:waypoint x="645" y="177" />
        <di:waypoint x="710" y="177" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="670" y="159" width="17" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_NotApproved_di" bpmnElement="Flow_NotApproved">
        <di:waypoint x="620" y="202" />
        <di:waypoint x="620" y="280" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="629" y="238" width="13" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ToGatewayReview_di" bpmnElement="Flow_ToGatewayReview">
        <di:waypoint x="570" y="320" />
        <di:waypoint x="505" y="320" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ReviewToApprove_di" bpmnElement="Flow_ReviewToApprove">
        <di:waypoint x="480" y="295" />
        <di:waypoint x="480" y="217" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="486" y="253" width="40" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ReviewToReject_di" bpmnElement="Flow_ReviewToReject">
        <di:waypoint x="455" y="320" />
        <di:waypoint x="338" y="320" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="377" y="302" width="57" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ToArchive_di" bpmnElement="Flow_ToArchive">
        <di:waypoint x="810" y="177" />
        <di:waypoint x="870" y="177" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ToEnd_di" bpmnElement="Flow_ToEnd">
        <di:waypoint x="970" y="177" />
        <di:waypoint x="1032" y="177" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
