<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_Dish"
             name="Dish Decision"
             namespace="http://operaton.org/examples/dish">
  
  <decision id="dish" name="Dish">
    <informationRequirement id="InformationRequirement_1">
      <requiredDecision href="#season" />
    </informationRequirement>
    <informationRequirement id="InformationRequirement_2">
      <requiredInput href="#guestCount" />
    </informationRequirement>
    <decisionTable id="DecisionTable_Dish" hitPolicy="UNIQUE">
      <input id="Input_Season" label="Season">
        <inputExpression id="InputExpression_Season" typeRef="string">
          <text>season</text>
        </inputExpression>
        <inputValues id="InputValues_Season">
          <text>"Fall","Winter","Spring","Summer"</text>
        </inputValues>
      </input>
      <input id="Input_GuestCount" label="Guest Count">
        <inputExpression id="InputExpression_GuestCount" typeRef="integer">
          <text>guestCount</text>
        </inputExpression>
      </input>
      <output id="Output_Dish" label="Dish" name="despisonDish" typeRef="string" />
      
      <rule id="Rule_1">
        <inputEntry id="InputEntry_Season_Fall"><text>"Fall"</text></inputEntry>
        <inputEntry id="InputEntry_GuestCount_LessEq8"><text>&lt;= 8</text></inputEntry>
        <outputEntry id="OutputEntry_Spareribs"><text>"Spareribs"</text></outputEntry>
      </rule>
      <rule id="Rule_2">
        <inputEntry id="InputEntry_Season_Winter"><text>"Winter"</text></inputEntry>
        <inputEntry id="InputEntry_GuestCount_LessEq8_2"><text>&lt;= 8</text></inputEntry>
        <outputEntry id="OutputEntry_Roastbeef"><text>"Roastbeef"</text></outputEntry>
      </rule>
      <rule id="Rule_3">
        <inputEntry id="InputEntry_Season_Spring"><text>"Spring"</text></inputEntry>
        <inputEntry id="InputEntry_GuestCount_LessEq4"><text>&lt;= 4</text></inputEntry>
        <outputEntry id="OutputEntry_DriedTomatoes"><text>"Dry Aged Gourmet Steak"</text></outputEntry>
      </rule>
      <rule id="Rule_4">
        <inputEntry id="InputEntry_Season_Spring_2"><text>"Spring"</text></inputEntry>
        <inputEntry id="InputEntry_GuestCount_5to8"><text>[5..8]</text></inputEntry>
        <outputEntry id="OutputEntry_Stew"><text>"Stew"</text></outputEntry>
      </rule>
      <rule id="Rule_5">
        <inputEntry id="InputEntry_Season_Summer"><text>"Summer"</text></inputEntry>
        <inputEntry id="InputEntry_GuestCount_LessEq10"><text>&lt;= 10</text></inputEntry>
        <outputEntry id="OutputEntry_LightSalad"><text>"Light Salad and a nice Steak"</text></outputEntry>
      </rule>
      <rule id="Rule_6">
        <inputEntry id="InputEntry_Season_Any"><text></text></inputEntry>
        <inputEntry id="InputEntry_GuestCount_Over10"><text>&gt; 10</text></inputEntry>
        <outputEntry id="OutputEntry_Beans"><text>"Bean Salad"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  
  <decision id="season" name="Season">
    <decisionTable id="DecisionTable_Season" hitPolicy="UNIQUE">
      <input id="Input_Temperature" label="Temperature">
        <inputExpression id="InputExpression_Temperature" typeRef="integer">
          <text>temperature</text>
        </inputExpression>
      </input>
      <output id="Output_Season" label="Season" name="season" typeRef="string">
        <outputValues id="OutputValues_Season">
          <text>"Fall","Winter","Spring","Summer"</text>
        </outputValues>
      </output>
      
      <rule id="Rule_Season_1">
        <inputEntry id="InputEntry_Cold"><text>&lt; 8</text></inputEntry>
        <outputEntry id="OutputEntry_Winter"><text>"Winter"</text></outputEntry>
      </rule>
      <rule id="Rule_Season_2">
        <inputEntry id="InputEntry_Cool"><text>[8..14]</text></inputEntry>
        <outputEntry id="OutputEntry_Spring"><text>"Spring"</text></outputEntry>
      </rule>
      <rule id="Rule_Season_3">
        <inputEntry id="InputEntry_Warm"><text>[15..24]</text></inputEntry>
        <outputEntry id="OutputEntry_Fall"><text>"Fall"</text></outputEntry>
      </rule>
      <rule id="Rule_Season_4">
        <inputEntry id="InputEntry_Hot"><text>&gt; 24</text></inputEntry>
        <outputEntry id="OutputEntry_Summer"><text>"Summer"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  
  <inputData id="guestCount" name="Guest Count" />
  
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_1">
      <dmndi:DMNShape id="DMNShape_Dish" dmnElementRef="dish">
        <dc:Bounds height="80" width="180" x="200" y="80" />
      </dmndi:DMNShape>
      <dmndi:DMNShape id="DMNShape_Season" dmnElementRef="season">
        <dc:Bounds height="80" width="180" x="80" y="220" />
      </dmndi:DMNShape>
      <dmndi:DMNShape id="DMNShape_GuestCount" dmnElementRef="guestCount">
        <dc:Bounds height="45" width="125" x="320" y="237" />
      </dmndi:DMNShape>
      <dmndi:DMNEdge id="DMNEdge_1" dmnElementRef="InformationRequirement_1">
        <di:waypoint x="170" y="220" />
        <di:waypoint x="245" y="160" />
      </dmndi:DMNEdge>
      <dmndi:DMNEdge id="DMNEdge_2" dmnElementRef="InformationRequirement_2">
        <di:waypoint x="382" y="237" />
        <di:waypoint x="335" y="160" />
      </dmndi:DMNEdge>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
